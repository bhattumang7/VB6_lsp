{
  "comments": {
    "lineComment": "'"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(Sub|Function|Property|Type|Enum|If|For|Do|While|Select|With)\\b",
      "end": "^\\s*(End Sub|End Function|End Property|End Type|End Enum|End If|Next|Loop|Wend|End Select|End With)\\b"
    }
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}
